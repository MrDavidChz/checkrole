{
    "name":"farbesofts/checkrole",
    "type":"library",
    "description":"A API Library  fro Laravel",
    "keywords":[
       "Laravel",
       "Library",
       "client"
    ],
    "license":"MIT",
    "authors":[
       {
          "name":"Fredy Rojas",
          "email":"farbe.soft@gmail.com",
          "role":"Developer"
       }
    ],
    "support":{
       "source":"https://github.com/farbesofts/checkrole",
       "issues":"https://github.com/farbesofts/checkrole/issues"
    },
    "require":{
      "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload":{
       "psr-4":{
          "Farbesofts\\Checkrole\\":"src/"
       }
    },
    "extra": {
      "branch-alias": {
          "dev-master": "1.0-dev"
      },
      "laravel": {
          "providers": [
              "Farbesofts\\Checkrole\\CheckroleServiceProvider"
          ],
          "aliases": {
              "Checkrole": "Farbesofts\\Checkrole\\Facades\\Checkrole"
          }
      }
  },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Farbesofts\\Checkrole\\test":"test"
        }
    }
 }